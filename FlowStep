public class FlowStep {
    private String text;
    private int level;

    public FlowStep(String text, int level) {
        this.text = "• ".concat(text);
        this.level = level;
    }

    public String getText() {
        return text;
    }

    public void setText(String text) {
        this.text = "• ".concat(text);
    }

    public int getLevel() {
        return level;
    }

    public void setLevel(int level) {
        this.level = level;
    }
}
